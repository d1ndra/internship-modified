   <script type="text/javascript">
       angular.module('test', []).controller('DemoCtrl', function ($scope, $http) {
    $scope.selectedTestAccount = null;
    $scope.clients = [];

    $http({
            method: 'GET',
            url: '/admin/getclients',
            data: { applicationId: 3 }
        }).success(function (result) {
        $scope.clients = result;
    });
});

